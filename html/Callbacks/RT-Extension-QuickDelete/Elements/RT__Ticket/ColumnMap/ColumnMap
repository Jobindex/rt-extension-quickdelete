<%ARGS>
$COLUMN_MAP => undef
</%ARGS>
<%init>
$COLUMN_MAP->{'QuickDelete'} = {
    title => ' ',
    value => sub {
        my $ticket = shift;
        return unless $ticket->CurrentUserHasRight('DeleteTicket');
        return $m->comp( '/RT-Extension-QuickDelete/Toggle',
            id => $ticket->id );
        }
};
</%init>

